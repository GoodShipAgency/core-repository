{
  "name": "mashbo/core-repository",
  "description": "Mashbo Core Repository package contains shared code relating to searching and filtering repositories",
  "minimum-stability": "stable",
  "license": "proprietary",
  "require": {
    "php": "^8.1",
    "doctrine/orm": "^2.13.3",
    "moneyphp/money": "^4",
    "symfony/http-foundation": "^5|^6",
    "league/csv": "^9.7",
    "iio/libmergepdf": "^4.0",
    "nesbot/carbon": "^2.63.0",
    "symfony/uid": "6.1.*",
    "symfony/messenger": "6.1.*",
    "symfony/framework-bundle": "6.1.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "psalm/plugin-phpunit": "^0.16.1",
    "psalm/plugin-symfony": "^3|^4",
    "vimeo/psalm": "5.0.0-rc1 as 4.30"
  },
  "autoload": {
    "psr-4": {
      "Mashbo\\CoreRepository\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  }
}
